{
  "name": "superagent-request-modifier",
  "version": "0.10.2",
  "description": "Patch superagent to add headers or queries to each request to a given host",
  "main": "lib/index.js",
  "author": {
    "name": "Gwilym Humphreys",
    "url": "https://github.com/gwilymhumphreys"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/founderlab/superagent-request-modifier"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./lib && ./node_modules/.bin/babel ./src --ignore '/node_modules/' --out-dir ./lib",
    "watch": "rm -rf ./lib && ./node_modules/.bin/babel ./src --ignore '/node_modules/' --watch --out-dir ./lib",
    "test": "eval $(cat test/.env) mocha test/**/*.tests.js"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "peerDependencies": {
    "superagent": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.0",
    "eslint": "^1.5.1",
    "eslint-config-founderlab": "^0.1.0",
    "expect": "^1.13.0",
    "mocha": "^2.2.5",
    "sinon": "^1.17.1"
  }
}
